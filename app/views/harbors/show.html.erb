<%- model_class = Harbor -%>
<div class="page-header">
  <h1><%=t '.title', :default => Harbor %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:process_id) %></dt>
      <dd><pre class="prettyprint"><%= @harbor.process_id %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:name) %></dt>
      <dd><pre class="prettyprint"><%= @harbor.name %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:subdomain) %></dt>
      <dd><pre class="prettyprint"><%= @harbor.subdomain %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:port) %></dt>
      <dd><pre class="prettyprint"><%= @harbor.port %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:password) %></dt>
      <dd><pre class="prettyprint"><%= @harbor.password %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:state) %></dt>
      <dd><pre class="prettyprint"><%= @harbor.state %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              harbors_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_harbor_path(@harbor), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              harbor_path(@harbor),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

